{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "no-console": "off",
    "linebreak-style": ["error", "windows"],
    "no-underscore-dangle": "off",
    "import/no-extraneous-dependencies": "off",
    "comma-dangle": "off",
    "operator-linebreak": "off",
    "implicit-arrow-linebreak": "off",
    "object-curly-newline": "off",
    "camelcase": "off"
  }
}
